<?xml version="1.0" encoding="UTF-8"?>
<project name="module_mpns-client" default="compile.module.mpns-client">
  <dirname property="module.mpns-client.basedir" file="${ant.file.module_mpns-client}"/>
  
  <property name="module.jdk.home.mpns-client" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.mpns-client" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.mpns-client" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.mpns-client" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="mpns-client.output.dir" value="${module.mpns-client.basedir}/target/classes"/>
  <property name="mpns-client.testoutput.dir" value="${module.mpns-client.basedir}/target/test-classes"/>
  
  <path id="mpns-client.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="mpns-client.module.production.classpath">
    <path refid="${module.jdk.classpath.mpns-client}"/>
  </path>
  
  <path id="mpns-client.runtime.production.module.classpath">
    <pathelement location="${mpns-client.output.dir}"/>
  </path>
  
  <path id="mpns-client.module.classpath">
    <path refid="${module.jdk.classpath.mpns-client}"/>
    <pathelement location="${mpns-client.output.dir}"/>
  </path>
  
  <path id="mpns-client.runtime.module.classpath">
    <pathelement location="${mpns-client.testoutput.dir}"/>
    <pathelement location="${mpns-client.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.mpns-client">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.mpns-client">
    <patternset refid="excluded.from.module.mpns-client"/>
  </patternset>
  
  
  <target name="compile.module.mpns-client" depends="compile.module.mpns-client.production,compile.module.mpns-client.tests" description="Compile module mpns-client"/>
  
  <target name="compile.module.mpns-client.production" depends="register.custom.compilers" description="Compile module mpns-client; production classes"/>
  
  <target name="compile.module.mpns-client.tests" depends="register.custom.compilers,compile.module.mpns-client.production" description="compile module mpns-client; test classes" unless="skip.tests"/>
  
  <target name="clean.module.mpns-client" description="cleanup module">
    <delete dir="${mpns-client.output.dir}"/>
    <delete dir="${mpns-client.testoutput.dir}"/>
  </target>
</project>